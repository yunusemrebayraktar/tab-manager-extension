{
    "manifest_version": 3,
    "name": "Tab Manager Extension",
    "description": "An extension to manage tabs when finding what you are looking for is hard.",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": ["storage", "tabs"],
    "background": { "service_worker": "background.js"},
    "content_scripts":{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }
}