{
    "manifest_version": 3,
    "name": "Movable Circle Tab Viewer",
    "version": "1.0",
    "permissions": ["tabs", "activeTab", "storage"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["style.css"]
      }
    ]
  }
  